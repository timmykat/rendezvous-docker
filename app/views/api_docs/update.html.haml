#terminal.modal.fade
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        .button{ type: 'button', class: 'close', 'data-dismiss' => 'modal', 'aria-hidden' => 'true'} &times;
        %h4{ class: 'modal-title' } Terminal Output
      .modal-body
        .window
          = @output
